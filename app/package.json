  "private": true,
  "scripts": {
    "postinstall": "yarn gen-types",
    "start:dev": "concurrently -n typegn,nextjs -c yellow,cyan \"yarn gen-types:watch\" \"next dev\"",
    "start:prod": "next start",
    "build": "next build",
    "lint": "prettier --write . --loglevel warn && eslint --fix . && tsc",
    "gen-types": "graphql-codegen --config codegen.yml",
    "gen-types:watch": "graphql-codegen --config codegen.yml --watch"
  },
  "dependencies": {
    "@emotion/babel-plugin": "11.3.0",
    "@emotion/react": "11.4.1",
    "@graphql-codegen/cli": "2.1.1",
