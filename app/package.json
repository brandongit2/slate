  "scripts": {
    "start:dev": "NODE_CONFIG_DIR=../config/ concurrently -n typegn,nextjs -c yellow,cyan \"nodemon -x \\\"yarn relay --customScalars.UUID=String\\\" -e ts --watch src/queries --watch ../api/schema.gql\" \"next dev\"",
    "start:prod": "next start",
    "build": "NODE_CONFIG_DIR=../config/ next build",
    "relay": "relay-compiler --src ./src/ --schema ../api/schema.gql --language typescript",
    "lint": "prettier --write . --loglevel warn && eslint --fix . && tsc"
  },
