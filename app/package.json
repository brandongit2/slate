    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "graphql": "15.5.1",
    "graphql-cli": "4.1.0",
    "next": "11.1.0",
    "nodemon": "2.0.12",
    "postcss": "8.3.6",
    "prettier": "2.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.12.2",
