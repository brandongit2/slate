{
  "name": "slate-api",
  "homepage": "https://slate.cx",
  "bugs": "https://github.com/brandongit2/slate/issues",
  "repository": "git@github.com:brandongit2/slate.git",
  "author": {
    "name": "Brandon Tsang",
    "email": "brandononline2@gmail.com",
    "url": "https://brandontsang.net"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "yarn cleandb:dev && NODE_CONFIG_DIR=../../config/ nest start --watch --preserveWatchOutput",
    "build": "nest build",
    "prod": "node dist/main",
    "test": "docker compose -f ../../docker-compose.test.yml up -d && yarn cleandb:test && NODE_CONFIG_DIR=../../config/ NODE_ENV=test jest --config=jest.config.js --runInBand --detectOpenHandles",
    "cleandb:dev": "NODE_CONFIG_DIR=../../../config/; cd knex && knex migrate:latest && knex seed:run",
    "cleandb:test": "NODE_CONFIG_DIR=../../../config/ NODE_ENV=test; cd knex && knex migrate:latest && knex seed:run",
    "psql": "docker exec -it slate-db psql -U postgres -d slate",
    "knex": "cd knex && NODE_CONFIG_DIR=../../../config/ knex --",
    "redis": "docker exec -it slate-sessions redis-cli",
    "lint": "prettier --write . --loglevel warn && eslint --fix . && tsc"
  },
  "dependencies": {
    "@brandonnpm2/nestjs-knex": "1.0.2",
    "@brandonnpm2/nestjs-redis": "1.0.2",
    "@golevelup/ts-jest": "0.3.1",
    "@nestjs/cli": "8.0.2",
    "@nestjs/common": "8.0.4",
    "@nestjs/core": "8.0.4",
    "@nestjs/graphql": "8.0.2",
    "@nestjs/platform-fastify": "8.0.4",
    "@nestjs/testing": "8.0.4",
    "@types/bcrypt": "5.0.0",
    "@types/faker": "^5.5.7",
    "@types/jest": "26.0.24",
    "@types/memoizee": "0.4.6",
    "@types/node": "16.4.1",
    "@types/uuid": "8.3.1",
    "apollo-server-core": "3.0.2",
    "apollo-server-fastify": "3.0.2",
    "bcrypt": "5.0.1",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "dayjs": "1.10.6",
    "eslint": "7.31.0",
    "faker": "5.5.3",
    "fastify-cookie": "5.3.1",
    "fastify-helmet": "5.3.2",
    "graphql": "15.5.1",
    "graphql-tools": "7.0.5",
    "ioredis": "4.27.6",
    "jest": "27.0.6",
    "knex": "0.95.7",
    "pg": "8.6.0",
    "prettier": "2.3.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0",
    "supertest": "6.1.4",
    "ts-jest": "27.0.4",
    "ts-node-dev": "1.1.8",
    "typescript": "4.3.5",
    "uuid": "8.3.2"
  }
}
