{
  "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-typescript"],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "#assets": "./src/assets",
          "#components": "./src/components",
          "#utils": "./src/utils"
        }
      }
    ]
  ],
  "ignore": ["**/*.story.tsx"]
}
