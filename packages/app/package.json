  "private": true,
  "scripts": {
    "dev": "NODE_CONFIG_DIR=../../config/ concurrently -n typegn,nextjs -c yellow,cyan \"nodemon -x \\\"yarn relay --customScalars.UUID=String\\\" -e ts --watch src/queries --watch ../api/schema.gql\" \"next dev\"",
    "build": "NODE_CONFIG_DIR=../../config/ next build",
    "export": "NODE_CONFIG_DIR=../../config/ next export",
    "start": "next start",
    "relay": "relay-compiler --src ./src/ --schema ../api/schema.gql --language typescript",
    "cap": "cap --",
    "lint": "prettier --write . --loglevel warn && eslint --fix . && tsc"
  },
  "dependencies": {
    "@capacitor/android": "3.1.1",
